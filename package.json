{
  "name": "kawax-js",
  "version": "0.0.1",
  "description": "React/Redux based framework",
  "author": "Paul Reboh",
  "license": "MIT",
  "homepage": "https://github.com/nakwa/kawax#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakwa/kawax.git"
  },
  "bugs": {
    "url": "https://github.com/nakwa/kawax/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "npm run nodemon",
    "lint": "eslint src/*.js",
    "lint:watch": "nodemon -e js -x 'npm run lint'",
    "nodemon": "nodemon --watch src -e js -x 'npm run build' --verbose",
    "postinstall": "npm run build",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --debug --watchAll"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "21.2.0",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.23.0",
    "babel-register": "6.26.0",
    "envify": "4.1.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "jest": "21.2.1",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "regenerator-runtime": "0.11.0"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.5",
    "deepmerge": "1.5.1",
    "history": "4.6.3",
    "immutable": "3.8.2",
    "key-mirror-nested": "1.2.4",
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-form": "1.3.0",
    "react-redux": "5.0.6",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2",
    "react-timeago": "3.4.3",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "request": "2.79.0",
    "request-promise-native": "1.0.4",
    "smart-class": "0.0.5",
    "url-pattern": "1.0.3",
    "uuid": "3.2.1"
  }
