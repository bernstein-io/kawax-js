
{
  "presets": [["env", { "modules": false, "loose": true }], "react"],
  "plugins": [
    "lodash",
    "transform-object-rest-spread",
    "transform-export-extensions",
    "transform-class-properties"
  ],
  "env": {
    "commonjs": {
      "presets": [["env", { "loose": true }]],
      "plugins": [
        "lodash",
        "transform-object-rest-spread",
        "transform-export-extensions",
        "transform-class-properties"
      ]
    }
  }
}
