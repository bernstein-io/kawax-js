
{
  "presets": ["react"],

  "env": {
    "es": {
      "presets": [["env", { "modules": false }]],
      "plugins": [
        "lodash",
        "transform-object-rest-spread",
        "transform-export-extensions",
        "transform-class-properties"
      ]
    },
    "commonjs": {
      "presets": [["env", { "modules": "commonjs"}]],
      "plugins": [
        "lodash",
        "transform-object-rest-spread",
        "transform-export-extensions",
        "transform-class-properties"
      ]
    }
  }
}
