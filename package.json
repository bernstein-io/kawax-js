{
  "name": "kawax-js",
  "version": "2.2.0",
  "description": "React/Redux based framework",
  "author": "Paul Reboh",
  "license": "MIT",
  "keywords": [
    "react",
    "redux"
  ],
  "homepage": "https://github.com/bernstein-io/kawax-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bernstein-io/kawax-js.git"
  },
  "bugs": {
    "url": "https://github.com/bernstein-io/kawax-js/issues"
  },
  "files": [
    "dist/cjs/**",
    "dist/es/**"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir dist/cjs",
    "build:es": "BABEL_ENV=es babel src --out-dir dist/es",
    "commonjs:watch": "npm run build:commonjs -- --watch",
    "es:watch": "npm run build:es -- --watch",
    "build": "npm run build:commonjs && npm run build:es",
    "watch": "npm run commonjs:watch & npm run es:watch",
    "dev": "npm run watch & npm run lint:watch",
    "lint": "eslint src tests --fix",
    "lint:watch": "nodemon -e js -x 'npm run lint'",
    "test": "jest --noStackTrace",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-syntax-export-default-from": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest-enzyme": "^7.1.2",
    "nodemon": "^2.0.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@loadable/component": "^5.12.0",
    "aphrodite": "^2.3.1",
    "babel-jest": "^26.3.0",
    "classnames": "^2.2.6",
    "clean-deep": "^3.0.2",
    "escape-regex-string": "^1.0.6",
    "history": "^4.10.1",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "lodash": "^4.17.20",
    "merge": "^1.2.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react-is": "^16.12.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "uuid": "^7.0.2",
    "warning": "^4.0.3"
  }
}
